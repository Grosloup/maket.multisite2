<!doctype html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% if block('title') %}{{ block('title') }}{%else %}Accueil{% endif %} | ZooParc de Beauval</title>
    {% block headLinks %}
        <link rel="stylesheet" href="/css/bootstrap.min.css"/>
        <link rel="stylesheet" href="/css/font-awesome.css"/>
    {% endblock %}
    {% block headStyles %}
        <style>
            body{
                padding-top: 50px;
            }
            #main-container{
                min-height: 700px;
            }
        </style>
    {% endblock %}
</head>
<body>
{{ render(controller("ZPBSitesZooBundle:Nav:mainTopbar")) }}
{% block page_header %}{% endblock %}
{{ render(controller("ZPBSitesZooBundle:Nav:secondNavbar")) }}
<div class="container" id="main-container">
    {% block main %}{% endblock %}
</div>



{{ render(controller("ZPBSitesZooBundle:Footer:main")) }}
{% block footScripts %}
    <script src="/js/vendor/jquery-2.1.1.min.js"></script>
    <script src="/js/vendor/bootstrap.min.js"></script>
{% endblock %}
</body>
</html>
